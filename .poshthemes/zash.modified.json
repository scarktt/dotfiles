{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ED8796",
          "style": "plain",
          "template": "スカル ",
          "type": "session"
        },
        {
          "foreground": "#A6DA95",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#7DC4E4",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#C6A0F6",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#EED49F>git(</>{{ .HEAD }}<#EED49F>)</>",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " \uf119",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
